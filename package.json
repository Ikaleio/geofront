{
	"name": "geofront-ts",
	"version": "0.5.1",
	"private": false,
	"description": "A high-performance Minecraft proxy core built with Rust and Bun FFI.",
	"main": "dist/geofront.js",
	"types": "dist/geofront.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "bun run build.mjs",
		"build:no-rust": "bun run build.mjs --no-rust",
		"dev:build": "cargo build",
		"dev": "NODE_ENV=development bun run",
		"dev:test": "NODE_ENV=development bun test --bail",
		"docs:dev": "vitepress dev docs",
		"docs:build": "vitepress build docs",
		"docs:start": "vitepress preview docs",
		"docs:clean": "rm -rf docs/.vitepress/dist docs/.vitepress/cache"
	},
	"devDependencies": {
		"@types/bun": "latest",
		"@types/uuid": "^10.0.0",
		"bun-types": "latest",
		"cpr": "^3.0.1",
		"socksv5": "^0.0.6",
		"typescript": "^5",
		"vitepress": "^1.3.0",
		"vitepress-plugin-llms": "^1.7.3"
	},
	"type": "module",
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"comlink": "^4.4.2",
		"mc-chat-format": "^1.2.2",
		"uuid": "^11.1.0",
		"zod": "^4.0.5"
	}
}
